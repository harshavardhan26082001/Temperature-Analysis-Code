MVI C,19H
LHLD 3000H
UP: PUSH H
INR H
INR L
DCR C
JNZ UP
;intialiasation till here
MVI C,19H
MVI B,31H
UP2:	POP D
	MOV A,D
	CPI 49H
	JZ DOWN
	DCR B
	MOV A,E
	CPI 49H
	JZ DOWN
	DCR B
	DCR C
	JNZ UP2
	MVI A,49H
	CMA
	STA 0ABCDH
	JMP VDOWN
DOWN:	MVI A,00H
	ADD B
	JPE DOWN2
	MVI A,00H
	JMP DOWN3
DOWN2:	MVI A,01H
DOWN3:	STA 0ABCDH
VDOWN:  hlt